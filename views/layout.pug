doctype html
html
  head
    title= pageTitle
    link(rel='stylesheet', href='/static/stylesheets/normalise.css')
    link(rel='stylesheet', href='/static/stylesheets/style.css')
  body
    .background(style=`background-image: url('/static/images/${image}.jpg');`)
      #wrapper.wrapper
        - var blub = colourScheme + "Colours";
        h1(class = blub)= heading
        block content
    include includes/footer.pug